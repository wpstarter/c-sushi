{
    "name": "wpstarter/c-sushi",
    "description": "Eloquent's missing \"array\" driver.",
    "license": "MIT",
    "authors": [
        {
            "name": "As247",
            "email": "as247@wpstarter.dev"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "wp-starter/database": "^1.0",
        "wp-starter/support": "^1.0"
    },
    "require-dev": {
        "doctrine/dbal": "^3.1.4",
        "wpstarter/o-testbench": "^1.0",
        "phpunit/phpunit": "^8.4 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sushi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
